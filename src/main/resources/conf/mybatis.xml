<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>
    <!--设置别名-->
    <typeAliases>
        <package name="com.iflytek.settings.domain"/>
        <package name="com.iflytek.workbench.domain"/>
    </typeAliases>


    <!--sql mapper(sql映射文件的位置)-->
    <mappers>
        <package name="com.iflytek.settings.dao"/>
        <package name="com.iflytek.workbench.dao"/>
    </mappers>
</configuration>